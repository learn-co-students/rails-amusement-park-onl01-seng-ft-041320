<div class="container">
  <%= form_with url:"/signup", method: :post do |f| %>
    <div class="form-group">
      <%= f.label :user_name, "Your name:" %>
      <%= f.collection_select :user_name, User.all, :id, :name, include_blank: "Please select your username from this dropdown" %>
    </div>
    <div class="form-group">
      <%= f.label :password, "Your password:" %>
      <br>
      <%= f.password_field :password, class: "form-control" %>
    </div>
    <div class="actions">
    <%= f.submit "Sign In" %>
    </div>
  <% end %>
</div>